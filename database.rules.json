{
  "rules": {
    "uNK": {
      ".read": "auth != null",
      ".write": "false"
    },
    "documents": {
      ".read": "false",
      ".write": "false"
    },
    "passwords": {
      ".read": "false",
      ".write": "false"
    },
    "a-user": {
      "$userID": {
        ".read": "auth.uid === $userID",
        ".write": "auth.uid === $userID"
      }
    }
  }
}